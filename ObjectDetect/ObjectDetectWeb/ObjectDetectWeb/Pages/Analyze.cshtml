@page "{fileName}"
@model ObjectDetectWeb.Pages.AnalyzeModel
@{
    ViewData["Title"] = "Анализ объектов";
}

<div class="text-center">
    <p>Обнаруженные объекты:</p>
    <table style="margin-left: auto;margin-right: auto;">
        <tr><th>Название объекта</th></tr>
        @foreach (var obj in Model.DetectObjects)
        {
                    <tr>
                        <td>@obj</td>
                    </tr>
        }
    </table>
    <img src="@Model.SourceImage" />
</div>
